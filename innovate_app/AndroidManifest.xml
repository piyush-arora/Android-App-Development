<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.innovate_app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.innovate_app.Main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    

    <activity
            android:name="com.example.innovate_app.Apple"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.innovate_app.APPLE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    
    
    
     <activity
            android:name="com.example.innovate_app.data"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.innovate_app.DATA" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    
    
    
  <activity
            android:name="com.example.innovate_app.mango"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.innovate_app.MANGO" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    
    
        
    
     <activity
            android:name="com.example.innovate_app.milk"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.innovate_app.MILK" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

     
          <activity
            android:name="com.example.innovate_app.water"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.innovate_app.WATER" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
    
        
</manifest>
